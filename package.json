{
  "name": "@jerome-benoit/sap-ai-provider",
  "version": "4.4.18",
  "type": "module",
  "description": "SAP AI Provider for Vercel AI SDK (powered by @sap-ai-sdk/orchestration and @sap-ai-sdk/foundation-models)",
  "keywords": [
    "ai",
    "vercel",
    "vercel-ai-sdk",
    "sap",
    "sap-ai-core",
    "sap-ai-sdk",
    "aicore",
    "btp",
    "cap",
    "agent",
    "orchestration"
  ],
  "homepage": "https://github.com/jerome-benoit/sap-ai-provider#readme",
  "bugs": {
    "url": "https://github.com/jerome-benoit/sap-ai-provider/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerome-benoit/sap-ai-provider.git"
  },
  "license": "Apache-2.0",
  "author": "jerome-benoit",
  "volta": {
    "node": "24.13.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:v2": "tsup --config tsup.config.v2.ts",
    "build:v2:watch": "tsup --config tsup.config.v2.ts --watch",
    "check-build": "ls -la dist/ && test -f dist/index.js && test -f dist/index.cjs && test -f dist/index.d.ts",
    "check-build:v2": "ls -la dist/ && test -f dist/index-v2.js && test -f dist/index-v2.cjs && test -f dist/index-v2.d.ts",
    "clean": "rm -rf dist",
    "lint": "npm run lint:md:all && eslint . --cache",
    "lint-fix": "npm run lint:md:fix && eslint . --fix --cache",
    "lint:md": "markdownlint -c .markdownlintrc '**/*.md'",
    "lint:md:all": "npm run lint:md && npm run lint:md:toc && npm run lint:md:mermaid",
    "lint:md:fix": "markdownlint -c .markdownlintrc '**/*.md' --fix",
    "lint:md:links": "find . -name '*.md' -not -path './node_modules/*' -exec markdown-link-check -c .markdown-link-check.json {} \\;",
    "lint:md:mermaid": "find . -name '*.md' -not -path './node_modules/*' -exec mermaid-validate validate-md {} \\; 2>&1 | grep -v 'No Mermaid code blocks'",
    "lint:md:toc": "tsx scripts/check-toc.ts",
    "prepare:v2": "tsx scripts/prepare-v2-package.ts",
    "prepublishOnly": "if [ \"$AI_SDK_VERSION\" = \"v2\" ]; then npm run build:v2 && npm run check-build:v2 && npm run prepare:v2; else npm run build && npm run check-build; fi",
    "prettier-check": "prettier --cache --check .",
    "prettier-fix": "prettier --cache --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:edge": "vitest --config vitest.edge.config.ts --run",
    "test:node": "vitest --config vitest.node.config.ts --run",
    "test:node:watch": "vitest --config vitest.node.config.ts",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.8",
    "@ai-sdk/provider-utils": "^4.0.15",
    "@sap-ai-sdk/foundation-models": "^2.7.0",
    "@sap-ai-sdk/orchestration": "^2.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv": "^17.3.1",
    "eslint": "^10.0.2",
    "eslint-plugin-jsdoc": "^62.7.0",
    "eslint-plugin-perfectionist": "^5.6.0",
    "globals": "^17.3.0",
    "markdown-link-check": "^3.14.2",
    "markdownlint-cli": "^0.47.0",
    "mermaid-validate": "^1.3.2",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "ai": "^5.0.0 || ^6.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
